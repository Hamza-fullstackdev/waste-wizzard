{
  "name": "waste-wizzards",
  "version": "1.0.0",
  "description": "waste-wizzard app",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon api/index.js",
    "start": "node api/index.js",
    "build":"npm install && npm install --prefix client && npm run build --prefix client",
    "deploy": "vercel --prod"
  }, "vercel": {
    "builds": [
      { "src": "api/**", "use": "@now/node" },
      { "src": "client/**", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/client/$1" }
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.1",
    "mongose": "^0.0.2-security",
    "nodemon": "^3.0.1"
  }
}
